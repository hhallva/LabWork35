<Window x:Class="Task5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="PrintButton" Content="Печать" Click="PrintButton_Click" Height="30" VerticalAlignment="Top"/>
        <Grid>
            <Canvas x:Name="printCanvas" >
                <Image Height="327" Canvas.Left="46" Source="Images/galaxy.jpg" Stretch="Fill" Canvas.Top="61" Width="136" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                <Image Height="260" Canvas.Left="240" Source="Images/background.jpg" Stretch="Fill" Canvas.Top="87" Width="550" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                <Image Height="155" Canvas.Left="438" Source="Images/plant.png" Stretch="Fill" Canvas.Top="192" Width="113" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            </Canvas>
        </Grid>
    </Grid>
</Window>
